<h1> Portfolio Items </h1>

<% @portfolio_items.each do |portfolio_item| %>
    <p><%= portfolio_item.title %></p>
    <p><%= portfolio_item.subtitle %></p>
    <p><%= portfolio_item.body %></p>
    <%= image_tag portfolio_item.thumb_image_  if !portfolio_item.thumb_image_.nil?%>
    <%= image_tag portfolio_item.main_image if !portfolio_item.thumb_image_.nil?%>


<% end %>